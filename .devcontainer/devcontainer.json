{
    "name": "Python 3.10 dev",
    "context": "..",
    "dockerFile": "Dockerfile",
    "settings": {
        "terminal.integrated.profiles.linux": {
            "zsh (login)": {
                "path": "zsh",
                "args": ["-l"]
            }
        },
        "python.pythonPath": "/usr/local/bin/python"
    },
    "extensions": [
        "ms-python.python",
        "kevinrose.vsc-python-indent"
    ]
}